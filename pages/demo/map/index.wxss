.map-tool{
	position: absolute;
	top:50rpx;
	right:20rpx;
	background:rgba(88,88,88,0.5);
	border-radius: 20rpx;
}
.zoom-tool{
	position: absolute;
	bottom:200rpx;
	right:20rpx;
	border-radius: 20rpx;
	background:rgba(88,88,88,0.5);
}
.def-tool{
	width:40rpx;
	height:40rpx;
	margin: 15rpx;
	border-radius: 5rpx;
	line-height: 40rpx;
	font-size: 30rpx;
	background:#ffffff;
}
.def-tool-btn{
	width:70rpx;
	height:70rpx;
	line-height: 70rpx;
	color:#4E82D7;
}
.position{
	width:50rpx;
	height:50rpx;
	margin:10rpx;
	line-height: 70rpx;
	font-size: 50rpx;
	border: 1rpx solid rgba(88,88,88,0.0);
	/*background: rgba(255,255,255,0.8);*/
}
.position:active{
	background: #ffffff;
}
.position:hover{
	border: 1rpx solid #ffffff;
}
.phone{
	width:55rpx;
	height:55rpx;
	margin: 10rpx 5rpx 10rpx 10rpx;
	line-height: 70rpx;
	font-size: 50rpx;
	border: 1rpx solid rgba(88,88,88,0.0);
}
.phone:active{
	background: #ffffff;
}
.phone:hover{
	border: 1rpx solid #ffffff;
}
.data-box{
	position: absolute;
	width:500rpx;
	left:0rpx;
	top:0rpx;
	background: rgba(88,88,88,0.5);
}
.data-hide-show{
	position: absolute;
	width:50rpx;
	height:50rpx;
	right:0rpx;
}