.controller{
	position:sticky;
	left: 25rpx;
	width: 700rpx;
	line-height: 100rpx;
	font-size:.7rem;
	/*color:#2a2a2a;*/
	background: rgba(18, 2, 39, 0.82);
	color:#fff;
}
.controller .month{
	width:255rpx;
	padding-left: 45rpx;
}
.controller .btn-def{
	display: inline-block;
	width: 150rpx;
	height: 50rpx;
	line-height: 50rpx;
	margin: 25rpx;
	border:1rpx solid #dbdbdb;
	border-radius: 25rpx;
	text-align:center;
}
.controller .btn-sel{
	border:1rpx solid #ff0000;
}
.calendar{
	position: sticky;
	width: 700rpx;
	left:25rpx;
	/*background: #fff;*/
	/*color:#2a2a2a;*/
	background: rgba(18, 2, 39, 0.82);
	color:#fff;
}
.swpier-box {
	height: 600rpx;
	width: 100%;
}
/* 非本月日期 */
.notCurrent {
	color: silver;
}
.today {
	color: #88a1fd;
}
.choice view{
	border-radius: 50%;
	background: #88a1fd;
	background-position:center;
	color: white;
}
.days-table {
	flex-wrap: wrap;
	align-content: flex-start;
}
.grid {
	width: 100rpx;
	height: 100rpx;
	text-align: center;
	line-height: 100rpx;
	font-size:.7rem;
}
.flex {
	display: flex;
}

/* 轴向 */
.column {
	flex-direction: column;
}
/* 主轴方向 */
.m-start {
	justify-content: flex-start;
}
.m-end {
	justify-content: flex-end;
}
.m-around {
	justify-content: space-around;
}
.m-between {
	justify-content: space-between;
}
.m-center {
	justify-content: center;
}
/* 侧轴方向 */
.s-center {
	align-items: center;
}

/* 提醒tip */
.tip-row{
	position:sticky;
	flex-wrap: wrap;
	left: 25rpx;
	width: 700rpx;
	font-size:.6rem;
	background: rgba(18, 2, 39, 0.82);
	color: #ffffff;
	border-top:1rpx solid #888888;
}
.tip{
	float: left;
	line-height: 50rpx;
	margin:auto 20rpx auto 20rpx;
}
.tip text{
	display:inline-block;
	width:20rpx;
	height:20rpx;
	background: #888888;
}

/* 日历中的tip */
.calendar-tip{
	margin-top:-20rpx;
	line-height: 0rpx;
}
.calendar-tip text{
	display:inline-block;
	width:20rpx;
	height:20rpx;
	border-radius: 10rpx;
	background: #888888;
}